{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "CONNECTION_STRING_IN_ENV_FILE"
  },
  "Jwt": {
    "Key": "JWT_SECRET_IN_ENV_FILE",
    "Issuer": "JWT_ISSUER_IN_ENV_FILE",
    "Audience": "JWT_AUDIENCE_IN_ENV_FILE",
    "DurationInMinutes": 120
  }
}
